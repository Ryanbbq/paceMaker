<script>

 


//Recipe Search Function
    function recipeSearch(){
        //alert("Clicked!");
      
        console.log("EDAMAME BEAN API BEGIN");
            
        //Clears the table's contents after each new search
        $("#label").html("");
        $("#image").html("");
        $("#source").html("");
        //add a hashtag
        $("#oneSpan").html("");

        //AJAX request from the BetterDoctor API https://api.betterdoctor.com
        $.ajax({
            //get method
            type: "get",
                
            //gets the url from the api 
            url: "https://api.edamam.com/search",
                
            //JSON, or JavaScript Object Notation, is a minimal, readable format for structuring data. 
            //It is used primarily to transmit data between a server and web application, as an alternative to XML.
            dataType: "json",
                
            // EDAMAM API
            data: {
                "q":$("#searchValue").val(),
                "app_key":"", // API Key PLEASE USE YOUR OWN
            },
        
            success: function(data,status) {
              console.log(data);
              console.log(data.length);
              for(var i = 0 ; i <5;i++){
              console.log(data["hits"][i]["recipe"]["label"]);
              $("#oneSpan").append("<br><div class='container' id='divOne'>" + "<h1><a href='" + data["hits"][i]["recipe"]["url"] + "'  target='_blank'>" + data["hits"][i]["recipe"]["label"] + "</a></h1>" + "<br>" +
                                  "<h1>Calories: " +  Math.round(data["hits"][i]["recipe"]["calories"])+"</h1><br>" +
                                  "<h1>Diet Labels: " + data["hits"][i]["recipe"]["dietLabels"] + "</h1><br>" + 
                             
                                  "<img src='" +data["hits"][i]["recipe"]["image"] + "'>" + "<br>" + "</div>")
                                  ;
       
       
          //    $("#label").append(data["hits"][i]["recipe"]["label"] + "<br>");
           //   $("#image").append("<img src='" +data["hits"][i]["recipe"]["image"] + "'>" + "<br>");     
            //  $("#source").append(data["hits"][i]["recipe"]["source"] + "<br>");
            //  $("#label").append("<a href='" + data["hits"][i]["recipe"]["url"] + "'>" + data["hits"][i]["recipe"]["label"] + "</a>" + "<br>");
              // console.log(data["q"]["hits"][1]["recipe"]["label"]);
            //  $("#calories").append(data["hits"][i]["recipe"]["calories"]+"<br>");
              console.log(data["hits"][i]["recipe"]["url"]);
              }
            },
        
            complete: function(data,status) { //optional, used for debugging purposes
         //   console.log("Debugger, error retrieving information..");
           // console.log(data);
             //   console.log(status);
            }
        
		});//AJAX

    } //End of recipeSearch() function.
        
</script>
  
