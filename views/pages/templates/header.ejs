          <ul class="nav navbar-nav navbar-right">
            <li id="loginLink"><a class= "nav-link" href="#" data-toggle='modal' data-target='#myModal'>Log In</a></li>
            <li id="signupLink"><a class= "nav-link" href="/userRegister">Sign Up</a></li>
            <li id="accSettings" class="hidden"><a id="accLink"> Account Settings</a></li>
            <li id="logoutLink" class="hidden"><a href="">Log Out</a></li>
          </ul>
      </div>
